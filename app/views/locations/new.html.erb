<%= form_for :location, url: locations_path do |f|%>
  <p>
    <%= f.label :city %><br>
    <%= f.text_field :city %>
  </p>
 
  <p>
    <%= f.label :placeName %><br>
    <%= f.text_area :placeName %>
  </p>

  <p>
    <%= f.label :latitude %><br>
    <%= f.text_field :latitude %>
  </p>

  <p>
    <%= f.label :altitude %><br>
    <%= f.text_field :altitude %>
  </p>

  <p>
    <%= f.label :country %><br>
    <%= f.text_field :country %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>